@charset "utf-8";

@-webkit-keyframes fadein { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@keyframes fadein { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@-webkit-keyframes fadeout { 
  0% { opacity: 1; }
  100% { opacity: 0; }
}

@keyframes fadeout { 
  0% { opacity: 1; }
  100% { opacity: 0; }
}

@-webkit-keyframes fave-starburst { 
  0% { background-position: 0px 0px; }
  100% { background-position: -456px 0px; }
}

@keyframes fave-starburst { 
  0% { background-position: 0px 0px; }
  100% { background-position: -456px 0px; }
}

@-webkit-keyframes fave-starburst-18px { 
  0% { background-position: 0px 0px; }
  100% { background-position: -342px 0px; }
}

@keyframes fave-starburst-18px { 
  0% { background-position: 0px 0px; }
  100% { background-position: -342px 0px; }
}

.fluid-avatar-editor-view { overflow: hidden; position: relative; }

.fluid-avatar-editor-view .avatar-background { opacity: 0.4; position: absolute; background-size: 100%; cursor: grab; transform: translateZ(0px); }

.fluid-avatar-editor-view .avatar-background.grabbing { cursor: grabbing; }

.fluid-avatar-editor-view .avatar-clip { border-radius: 50%; overflow: hidden; width: 400px; height: 400px; position: absolute; margin: 0px auto; top: calc(50% - 200px); left: calc(50% - 200px); pointer-events: none; }

@media (max-width: 812px) and (orientation: landscape), only screen and (max-width: 700px) and (orientation: portrait) {
  .fluid-avatar-editor-view .avatar-clip { width: 200px; height: 200px; top: calc(50% - 100px); left: calc(50% - 100px); }
}

.fluid-avatar-editor-view .avatar-image { background-size: 100%; position: absolute; }

.fluid-avatar-editor-view .controls-area { position: absolute; bottom: 30px; left: 50%; margin-left: -215px; height: 48px; opacity: 1; padding-top: 0px; width: 430px; background-color: rgba(0, 0, 0, 0.6); border-radius: 3px; z-index: 1; transition: opacity 0.25s ease-out 0s; }

.fluid-avatar-editor-view .controls-area.clickthrough { pointer-events: none; }

@media (max-width: 812px) and (orientation: landscape), only screen and (max-width: 700px) and (orientation: portrait) {
  .fluid-avatar-editor-view .controls-area { margin-left: 0px; width: 300px; transform: translateX(-50%); }
}

.fluid-avatar-editor-view .zoom-controls { opacity: 1; pointer-events: none; position: absolute; top: 16px; left: 34px; width: 360px; height: 15px; margin: 0px auto; }

.fluid-avatar-editor-view .zoom-controls .zoom-handle { position: absolute; pointer-events: auto; left: 180px; top: 0px; z-index: 1; width: 15px; height: 15px; border-radius: 18px; transition: none 0s ease 0s; background-color: rgba(255, 255, 255, 0.95); cursor: grab; }

.fluid-avatar-editor-view .zoom-controls .zoom-handle.dragging { cursor: grabbing; }

.fluid-avatar-editor-view .zoom-controls .zoom-guide { position: relative; top: 5px; background-color: rgba(255, 255, 255, 0.5); border-radius: 6px; height: 4px; }

.fluid-avatar-editor-view .zoom-controls .zoom-guide .low-quality-zone { height: 4px; position: absolute; left: 0px; top: 0px; background-color: rgba(0, 0, 0, 0.6); }

.fluid-avatar-editor-view .zoom-controls p { position: absolute; left: -24px; margin: 0px; padding: 0px; font-size: 14px; color: rgb(255, 255, 255); text-shadow: rgb(0, 0, 0) 1px 1px 1px; cursor: pointer; pointer-events: auto; }

.fluid-avatar-editor-view .zoom-controls p:nth-child(2) { left: auto; right: -24px; }

.fluid-avatar-editor-view .zoom-controls p.zoom-minus, .fluid-avatar-editor-view .zoom-controls p.zoom-plus { top: -1px; }

@media (max-width: 812px) and (orientation: landscape), only screen and (max-width: 700px) and (orientation: portrait) {
  .fluid-avatar-editor-view .zoom-controls { width: 230px; }
  .fluid-avatar-editor-view .zoom-controls .zoom-handle { left: 50%; }
}

@-webkit-keyframes pinkBall { 
  0% { z-index: 1; }
  50% { transform: translateX(12px); }
}

@keyframes pinkBall { 
  0% { z-index: 1; }
  50% { transform: translateX(12px); }
}

@-webkit-keyframes blueBall { 
  50% { transform: translateX(-12px); }
}

@keyframes blueBall { 
  50% { transform: translateX(-12px); }
}

.fluid .balls { position: relative; width: 24px; height: 12px; margin: 0px auto; display: inline-block; }

.fluid .balls::after, .fluid .balls::before { position: absolute; top: 0px; width: 12px; height: 12px; content: ""; border-radius: 100%; animation: 1.6s ease-in-out 0s infinite normal none running pinkBall; }

.fluid .balls::before { background: rgb(255, 50, 156); left: 0px; }

.fluid .balls::after { animation: 1.6s ease-in-out 0s infinite normal none running blueBall; background: rgb(0, 99, 220); right: 0px; }

.fluid .photo_container { position: relative; line-height: 0; display: inline-block; }

.fluid .photo_container .pc_link { position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%); width: 50px; height: 50px; border: 2px solid rgb(255, 255, 255); box-sizing: border-box; background-image: url("https://combo.staticflickr.com/ap/build/images/video.svg"); background-repeat: no-repeat; background-position: 50% center; background-size: 40%; border-radius: 50%; filter: brightness(0) invert(1); }

.fluid .photo_container .pc_link img { display: none; }

.fluid .bbml_set h4 { margin: 0px; font-size: 16px; color: rgb(0, 109, 172); }

.fluid .bbml_set .SetCase { position: relative; }

.fluid .bbml_set .SetCase h4 { font-size: 14px; font-weight: 400; margin-top: -10px; }

.fluid .bbml_set .SetCase .setCommentLinkDiv { position: absolute; bottom: 10px; left: 5px; display: none; }

.fluid .bbml_set .SetCase .setCommentLinkDiv > img { display: none; margin: 0px; }

.fluid .bbml_set .SetCase .setLink img { width: 80px; height: 80px; margin: 0px; }

.fluid .chalkboard { z-index: 2006; width: 60%; min-width: 1060px; max-width: 100%; position: absolute; top: 0px; left: 0px; margin: 0px auto; box-sizing: border-box; }

